@model HomeMoneyVM

<div class="col-lg-6 col-md-12 col-6 mb-4">
    <div class="card">
        <div class="card-body">
            <div class="card-title d-flex align-items-start justify-content-between">
               <a asp-controller="Profit" asp-action="Index">
                    <div class="avatar flex-shrink-0">
                        <img src="../assets/img/icons/unicons/chart-success.png" alt="Credit Card" class="rounded" />
                    </div>
               </a>
            </div>
            <span>Gəlirlər</span>
            <h5 class="card-title text-nowrap mb-1">@Model.TotalProfit Azn</h5>
        </div>
    </div>
</div>

<div class="col-lg-6 col-md-12 col-6 mb-4">
    <div class="card">
        <div class="card-body">
            <div class="card-title d-flex align-items-start justify-content-between">
                <a asp-controller="Cost" asp-action="Index">
                    <div class="avatar flex-shrink-0">
                        <img src="../assets/img/icons/unicons/wallet-info.png" alt="Credit Card" class="rounded" />
                    </div>
                </a>
            </div>
            <span>Xərclər</span>
            <h5 class="card-title text-nowrap mb-1">@Model.TotalCost Azn</h5>
        </div>
    </div>
</div>